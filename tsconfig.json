{
  "compilerOptions": {
      "noEmit": true,
      "target": "ESNext",
      "module": "ESNext",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "jsx": "preserve",
      "types": ["vite/client", "node"],
      "baseUrl": ".",
      "outDir": "ts-out",
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "skipLibCheck": true
  }
}
